<%- include('../partials/header.ejs') %>
<form action="/notifications/<%= foundNotif._id %>?_method=patch" method="post">
    <input type="text" name="title" id="" value="<%= foundNotif.title %>">
    <input type="text" name="body" id="" value="<%= foundNotif.body %>">
    <input type="text" name="author" id="" value="<%= foundNotif.author %>">
    <input type="submit" value="Update">
</form>
<form action="/notifications/<%= foundNotif._id %>?_method=delete" method="post">
<input type="submit" value="Delete">
</form>
